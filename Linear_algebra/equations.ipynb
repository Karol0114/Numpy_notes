   "source": [
    "# Wektor wyrazów wolnych:\n",
    "B = np.array([[10], [-1]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "e276372e",
   "metadata": {},
   "outputs": [
    {
