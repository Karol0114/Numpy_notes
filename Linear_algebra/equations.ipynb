   "source": [
    "#Rozwiązujemy układ równań:\n",
    "# { 2x + 4y = 10\n",
    "# { x - y = -1\n",
    "\n",
    "#Macierz współczynników:\n",
    "A = np.array([[2, 4], [1, -1]])\n"
   ]
  },
